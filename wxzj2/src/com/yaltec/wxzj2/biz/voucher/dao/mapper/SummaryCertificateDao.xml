<!-- 
		namespace：必须与对应的接口全类名一致
		id:必须与对应接口的某个对应的方法名一致
	 -->
<mapper namespace="com.yaltec.wxzj2.biz.voucher.dao.SummaryCertificateDao">
	
	<!--查询会计科目信息-->
	<select id="findAll" parameterType="java.util.Map" statementType="CALLABLE" 
		resultType="com.yaltec.wxzj2.biz.voucher.entity.SummaryCertificate" >
	    <![CDATA[
            {call P_SordineSumByVoucher(
                #{begindate,javaType=java.lang.String,jdbcType=DATE,mode=IN},
                #{enddate,javaType=java.lang.String,jdbcType=DATE,mode=IN},
                #{hzlb,javaType=java.lang.String,jdbcType=SMALLINT,mode=IN}
            )}
        ]]>
	</select>
	
	<!--查询会计科目信息-->
	<select id="findSummaryCertificate" parameterType="java.util.Map" statementType="CALLABLE" 
		resultType="com.yaltec.wxzj2.biz.voucher.entity.SummaryCertificate" >
	    <![CDATA[
            {call P_SordineSumByVoucher(
                #{begindate,javaType=java.lang.String,jdbcType=DATE,mode=IN},
                #{enddate,javaType=java.lang.String,jdbcType=DATE,mode=IN},
                #{hzlb,javaType=java.lang.String,jdbcType=SMALLINT,mode=IN}
            )}
        ]]>
	</select>
	
</mapper>