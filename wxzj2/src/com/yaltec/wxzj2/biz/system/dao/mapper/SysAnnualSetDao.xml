<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 
		namespace：必须与对应的接口全类名一致
		id:必须与对应接口的某个对应的方法名一致
	 -->
<mapper namespace="com.yaltec.wxzj2.biz.system.dao.SysAnnualSetDao">
	
	 <!-- 获取账务月度 -->
    <select id="find" resultType="com.yaltec.wxzj2.biz.system.entity.SysAnnualSet">
    	select top 1 * from SordineAnnual
    </select>
    
    <!-- 更新系统财务年度 -->
    <select id="update" parameterType="java.util.Map" statementType="CALLABLE">
		<![CDATA[
	        {call P_saveAnnual(
	            #{bdate,javaType=java.lang.String,jdbcType=DATE,mode=IN},
	            #{edate,javaType=java.lang.String,jdbcType=DATE,mode=IN},
	            #{cwdate,javaType=java.lang.String,jdbcType=DATE,mode=IN},
	            #{result,javaType=java.lang.String,jdbcType=SMALLINT,mode=OUT}
	        )}
	    ]]>
	</select>  
</mapper>