<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 
		namespace：必须与对应的接口全类名一致
		id:必须与对应接口的某个对应的方法名一致
	 -->
<mapper namespace="com.yaltec.wxzj2.biz.system.dao.PrintConfigDao">	
	
	<select id="get" resultType="com.yaltec.wxzj2.biz.system.entity.ConfigPrint" parameterType="String">
	<![CDATA[
		select * from ConfigPrint where moduleKey=#{key} order by property
	]]>
	</select>
     
</mapper>